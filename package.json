{
  "name": "create-drippy-api",
  "version": "0.0.4",
  "description": "Create a typescript based api with a single command",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barrybtw/create-drippy-api.git"
  },
  "keywords": [
    "api",
    "typescript",
    "orm",
    "backend",
    "simplicity"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-drippy-api": "dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:latest": "pnpm build && npm publish"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.2",
    "@types/node": "^20.4.6",
    "tsup": "7.1.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "execa": "^7.2.0",
    "fs-extra": "^11.1.1",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.10",
    "ora": "^7.0.1"
  }
}
